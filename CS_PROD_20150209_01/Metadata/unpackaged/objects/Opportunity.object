    </validationRules>
    <validationRules>
        <fullName>At_Risk_Required_6_Months_Before_Renewal</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
RecordType.DeveloperName = &quot;Renewals&quot;,
ISPICKVAL(At_Risk__c,&quot;&quot;), 
(Renewal_Due_Date__c-TODAY())&lt;=180,
NOT($Profile.Name=&quot;System Administrator&quot;)
)</errorConditionFormula>
        <errorDisplayField>At_Risk__c</errorDisplayField>
        <errorMessage>At Risk? is required 6 Months before Renewal Due Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Stage_Requirements</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(StageName)=TRUE,
NOT($Profile.Name=&quot;Billing&quot;),
ISPICKVAL(StageName, &quot;Finance: Rejected&quot;)
)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Only Billing can change the Stage to Finance: Rejected</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Both_Stage_and_Opp_Status_Voided</fullName>
        <active>true</active>
        <description>Enforces that both Stage and Opp Status are set to Voided, if one of them is changed to Voided.</description>
        <errorConditionFormula>OR(AND(ISPICKVAL( StageName , &quot;Voided&quot;), NOT(ISPICKVAL( Opportunity_Status__c  , &quot;Voided&quot;))) ,

AND(NOT(ISPICKVAL( StageName , &quot;Voided&quot;)), ISPICKVAL( Opportunity_Status__c  , &quot;Voided&quot;)) )</errorConditionFormula>
        <errorDisplayField>Access_to_Decision_Maker__c</errorDisplayField>
        <errorMessage>Both the Stage and Opp Status need to be set to Voided if you are voiding this Opp.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Change_Opp_Stage</fullName>
        <active>true</active>
        <description>Certain Opportunity Stages may only be changed by Billing, Contract Admin, and Sys Admin</description>
        <errorConditionFormula>AND(ISCHANGED(StageName)=TRUE, 
OR( ISPICKVAL( StageName, &quot;APP: Pending Approval&quot;), 
ISPICKVAL( StageName , &quot;APP: Approved&quot;), 
ISPICKVAL( StageName, &quot;Contract Sent&quot;), 
ISPICKVAL( StageName, &quot;Cust: Contract Received&quot;), 
ISPICKVAL( StageName, &quot;Cust: Contract Opened&quot;), 
ISPICKVAL( StageName, &quot;Post APP: Pending Approval&quot;), 
ISPICKVAL( StageName, &quot;Finance: Tax Exempt Review&quot;), 
ISPICKVAL( StageName, &quot;CA: Review&quot;), 
ISPICKVAL( StageName, &quot;CA: Early Renewal&quot;), 
ISPICKVAL( StageName, &quot;Closed Won&quot;), 
ISPICKVAL( StageName, &quot;Closed Lost&quot;), 
ISPICKVAL(StageName, &quot;Voided&quot;), 
ISPICKVAL( PRIORVALUE(StageName), &quot;APP: Pending Approval&quot;), 
ISPICKVAL( PRIORVALUE(StageName) , &quot;APP: Approved&quot;), 
ISPICKVAL( PRIORVALUE(StageName), &quot;Contract Sent&quot;), 
ISPICKVAL( PRIORVALUE(StageName), &quot;Cust: Contract Received&quot;), 
ISPICKVAL( PRIORVALUE(StageName), &quot;Cust: Contract Opened&quot;), 
ISPICKVAL( PRIORVALUE(StageName), &quot;Post APP: Pending Approval&quot;), 
ISPICKVAL( PRIORVALUE(StageName), &quot;Finance: Tax Exempt Review&quot;), 
ISPICKVAL( PRIORVALUE(StageName), &quot;CA: Review&quot;), 
ISPICKVAL( PRIORVALUE(StageName), &quot;CA: Early Renewal&quot;), 
ISPICKVAL( PRIORVALUE(StageName), &quot;Closed Won&quot;), 
ISPICKVAL( PRIORVALUE(StageName), &quot;Closed Lost&quot;), 
ISPICKVAL( PRIORVALUE(StageName), &quot;Voided&quot;)), 
NOT($Profile.Name=&quot;Billing&quot;), 
NOT($Profile.Name=&quot;Contract Administration&quot;), 
NOT($Profile.Name=&quot;System Administrator&quot;), 
NOT( $User.Bypass_Validation__c ))</errorConditionFormula>
        <errorMessage>The Stage Name of this Opportunity can only be Changed by &apos;Billing&apos; or &apos;Contract Administration&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_Document_Approved</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL(StageName, &quot;Contract Approved&quot;),
 NOT( RecordType.DeveloperName = &quot;Document_Changes_Approved&quot; )
)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Document changes must be approved before moving to this stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Clone_Check_Opp_Status</fullName>
        <active>true</active>
        <description>Checks that Opportunity Status is Pending Sale after a Clone</description>
        <errorConditionFormula>AND( 
ISNEW(), 
RecordTypeId =&quot;Transactional Broadcast&quot;,
NOT(ISPICKVAL(Opportunity_Status__c,&quot;Pending Sale&quot;))
 )</errorConditionFormula>
        <errorDisplayField>Opportunity_Status__c</errorDisplayField>
        <errorMessage>Opportunity Status can only be Pending Sale on your first Save.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Clone_Check_PinPoint_Notes</fullName>
        <active>false</active>
