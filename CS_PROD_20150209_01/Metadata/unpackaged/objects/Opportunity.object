        <errorConditionFormula>SBQQ__QuoteCount__c &lt;&gt; PriorValue(SBQQ__QuoteCount__c) &amp;&amp;( ISPICKVAL(StageName,&apos;APP: Approved&apos;) || ISPICKVAL(StageName,&apos;Closed Won&apos;) || ISPICKVAL(StageName,&apos;CA: Review&apos;) || ISPICKVAL(StageName,&apos;Closed Lost&apos;) || ISPICKVAL(StageName, &apos;Voided&apos;)) &amp;&amp; ( $Profile.Name &lt;&gt; &apos;Billing&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;Contract Administration&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;Sales Support&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;System Administrator&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;Renewals Sales User&apos;) &amp;&amp;  $User.Bypass_Validation__c = FALSE</errorConditionFormula>
