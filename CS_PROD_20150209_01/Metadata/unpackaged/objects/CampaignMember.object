    <validationRules>
        <fullName>Non_Marketing_Adds</fullName>
        <active>false</active>
        <errorConditionFormula>AND(NOT($Profile.Name = &quot;Marketing&quot;),
OR(
NOT(ISPICKVAL(Campaign.Type, &quot;Nurture&quot;)),
NOT(ISPICKVAL(Status, &quot;Sent&quot;))
),
OR($User.Bypass_Validation__c == false)
)</errorConditionFormula>
        <errorMessage>Non-Marketing Users can only add someone to a Nurture Campaign with a Status of Sent.  Please correct your addition or contact Marketing for help.</errorMessage>
    </validationRules>
