    <validationRules>
        <fullName>Annual_Subscription_Terms_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
SBQQ__SubscriptionTerm__c&lt;12, 
ISPICKVAL(Payment_Plan__c, &quot;Annual&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Payment_Plan__c</errorDisplayField>
        <errorMessage>Annual Payment Plans can not be selected for Subscription Terms of less than 12 months.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Card_Length_Amex</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(LEN(Card_Number__c)=15), 
ISPICKVAL(Card_Type__c, &apos;Amex&apos;)
)</errorConditionFormula>
        <errorDisplayField>Card_Number__c</errorDisplayField>
        <errorMessage>Amex Card Number must be 15 characters long</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Card_Length_NonAmex</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(LEN(Card_Number__c)=16), 
OR(ISPICKVAL(Card_Type__c, &apos;Visa&apos;),
ISPICKVAL(Card_Type__c, &apos;Discover&apos;),
ISPICKVAL(Card_Type__c, &apos;MasterCard&apos;))
)</errorConditionFormula>
        <errorDisplayField>Card_Number__c</errorDisplayField>
        <errorMessage>Card Number must be 16 characters long for Visa, Discover, and MasterCard.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>High_Risk_credit_with_out_credit_card</fullName>
        <active>true</active>
        <description>This trigger prevents users from processing a quote with credit rating =high risk and payment type not = credit card</description>
        <errorConditionFormula>AND(  NOT( ISPICKVAL(Payment_Plan__c, &quot;Credit Card&quot;)),    Credit_Risk__c = &quot;High Risk&quot;)</errorConditionFormula>
        <errorMessage>You cannot save a High Risk customer without a Credit Card.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Contract_with_Use_Existing</fullName>
        <active>true</active>
        <description>Require that the Master Contract is filled in when someone checks Use Existing Contract</description>
        <errorConditionFormula>AND(
ISBLANK(SBQQ__MasterContract__c),
STX_Use_Existing_Contract_Number__c=TRUE
)</errorConditionFormula>
        <errorDisplayField>SBQQ__MasterContract__c</errorDisplayField>
        <errorMessage>You must have a Master Contract selected when checking STX: Use Existing Contract Number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Line_Item_edits_For_Recordtype</fullName>
        <active>true</active>
        <description>Restrict line item changes when the record type is After Approvals,Post Signature or Closed</description>
        <errorConditionFormula>(NOT(ISNEW()) &amp;&amp;  Opp_Stage__c =&apos;Closed Won&apos;) &amp;&amp; ( $User.Bypass_Validation__c = FALSE &amp;&amp; $Profile.Name &lt;&gt; &apos;Billing&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;Contract Administration&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;Sales Support&apos; &amp;&amp; $Profile.Name &lt;&gt; &apos;System Administrator&apos;)</errorConditionFormula>
        <errorMessage>You cannot delete or change line Items on an Approved quote</errorMessage>
    </validationRules>
