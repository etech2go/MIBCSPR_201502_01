    <fields>
        <fullName>Has_Option_Discount__c</fullName>
        <externalId>false</externalId>
        <formula>/* Returns 1 if this product is a component of a package, and it has a pre-defined discount at the Product Option level; otherwise, returns 0 */

IF( NOT( ISBLANK( SBQQ__ProductOption__r.SBQQ__DiscountAmount__c ) ) , 1 , 0 )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Has Option Discount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Indented_Product_Name__c</fullName>
        <externalId>false</externalId>
        <formula>LPAD( SBQQ__ProductName__c , LEN( SBQQ__ProductName__c ) + (4 * SBQQ__OptionLevel__c ) , &quot; &quot; )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Product Name</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Is_Monitoring_Service_A_La_Carte__c</fullName>
        <externalId>false</externalId>
        <formula>/* Returns 1 if the product is a monitoring service added &quot;a la carte&quot;, i.e. not as part of a bundle; returns 0 otherwise */

IF( 
    SBQQ__ProductFamily__c = &quot;Monitoring Services&quot; &amp;&amp; 
    (ISBLANK( SBQQ__RequiredBy__c ) || ISNULL( SBQQ__RequiredBy__c ) ) ,

    /* This is not a component of a bundle */
    1,

    /* All other cases */
    0
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Monitoring Service A La Carte</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Is_Monitoring_or_Broadcasting_Services__c</fullName>
        <externalId>false</externalId>
        <formula>IF (ProductFamily2__c = &quot;Monitoring Services&quot; || ProductFamily2__c = &quot;Broadcasting Services&quot;,
  TRUE,
  FALSE
)</formula>
        <label>Is Monitoring or Broadcasting Services</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Plan_Services__c</fullName>
        <externalId>false</externalId>
        <formula>IF (ProductFamily2__c = &quot;Plan Services&quot;, 
TRUE, 
FALSE 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
