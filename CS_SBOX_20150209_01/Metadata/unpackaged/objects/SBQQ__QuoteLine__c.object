        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Bulleted_Product_Name__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Quantity_Whole_Number__c &lt; 2 , 
    &quot;•&quot; + &quot; &quot; + SBQQ__ProductName__c , 
    &quot;•&quot; + &quot; &quot; + TEXT( Quantity_Whole_Number__c ) + &quot; &quot; + SBQQ__ProductName__c 
)</formula>
        <label>Bulleted Product Name</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Calc_HD_Clip_Promo_Price__c</fullName>
        <externalId>false</externalId>
        <formula>IF (MOD( SBQQ__Quantity__c, 2) = 0 ,

  /* Even quantity */
  ((SBQQ__ListPrice__c * SBQQ__Quantity__c / 2) + ( (SBQQ__ListPrice__c - 75) * SBQQ__Quantity__c / 2 )) / SBQQ__Quantity__c ,


  /* Odd quantity */
  ((SBQQ__ListPrice__c * (SBQQ__Quantity__c - 1) / 2) + ( (SBQQ__ListPrice__c - 75) * (SBQQ__Quantity__c - 1) / 2 ) + SBQQ__ListPrice__c ) / SBQQ__Quantity__c

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Calc HD Clip Promo Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
